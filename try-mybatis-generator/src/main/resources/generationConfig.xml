<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="MySQLTables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/try_java?useUnicode=true"
        userId="root"
        password="root">
    </jdbcConnection>

    <javaModelGenerator targetPackage="com.example.demo.domain" targetProject="try-mybatis-generator/src/main/java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.example.demo.xml"  targetProject="try-mybatis-generator/src/main/java">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="com.example.demo.mapper"  targetProject="try-mybatis-generator/src/main/java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table schema="try_java" tableName="try_mybatis_generator">
      <property name="useActualColumnNames" value="true"/>
    </table>

  </context>
</generatorConfiguration>

